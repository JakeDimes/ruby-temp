

<% if flash[:login_error]%>

  <div class = "flash"><%=flash[:login_error]%></div>

<%end %>

<%= form_with url: session_password_path, method: :get do |form| %>

  <%= form.label :entered_uname, 'Enter Email' %>
  <%= form.text_field :entered_uname %>
  <%= form.submit name: :label, value: "Next" %>

<%end %>